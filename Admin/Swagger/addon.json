{
    "swagger": "2.0",
    "info": {
      "version": "1.0.0",
      "title": "Admin Api",
      "description": "Admin API Microservice",
      "contact": {
        "name": "Madhuri Gavade",
        "url": "https://opensource.org/licenses/MIT"
      }
    },
    "host": "localhost:3000",
    "basePath": "/",
    "tags": [
      {
        "name": "addon",
        "description": "addon API"
      }
    ],
    "paths": {
      "/addon": {
        "get": {
          "tags": [
            "addon"
          ],
          "summary": "Get all addons in system",
          "responses": {
            "200": {
              "description": "Get all addon from the system"         
            }
          }
        },
        "post": {
          "tags": [
            "addon"
          ],
          "description": "Create new addon in system",
          "parameters": [
            {
              "name": "user",
              "in": "body",
              "description": "addon that we want to create"
              
            }
          ],
          "produces": [
            "application/json"
          ],
          "responses": {
            "200": {
              "description": "New addon is created"
              
            }
          }
        }
      },
      "/addon/{id}": {
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "ID of addon that we want to find",
            "type": "string"
          }
        ],
        "get": {
          "tags": [
            "addon"
          ],
          "summary": "Get single addon in system",
          "responses": {
            "200": {
              "description": "All addon are here"
              
            }
          }
        },
        "put": {
          "parameters": [
            {
              "name": "addon",
              "in": "body",
              "description": "addon with new values of properties"
              
            }
          ],
          "tags": [
            "addon"
          ],
          "summary": "Get all Update caraddon in system",
          "responses": {
            "200": {
              "description": "Update Caraddon"
              
            }
          }
        },
        "delete": {
          "summary": "Delete caraddon with given ID",
          "tags": [
            "addon"
          ],
          "responses": {
            "200": {
              "description": "addon is deleted"
              
            }
          }
        }
      }
    }
  }